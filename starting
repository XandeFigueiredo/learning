if (anoDeNascimento <= 1945) {
  resultado = "Geração silenciosa"
} else if (anoDeNascimento <= 1964) {
  resultado = "Boomers"
} else if (anoDeNascimento <= 1980) {
  resultado = "Geração X"
} else if (anoDeNascimento <= 1996) {
  resultado = "Millennials"
} else {
  resultado = "Geração Z"
